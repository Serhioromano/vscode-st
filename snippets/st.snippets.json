	"FUNCTION ... END_FUNCTION": {
		"prefix": "function ",
		"body": [
			"FUNCTION ${1:Name} : ${2:Type}",
			"\t$0",
			"END_FUNCTION"
		],
		"description": "Define FUNCTION"
	},
	"FUNCTION_BLOCK ... END_FUNCTION_BLOCK": {
		"prefix": "function_block",
		"body": [
