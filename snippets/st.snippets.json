	"INITIAL_STEP …": {
		"prefix": "initial_step ",
		"body": [
			"INITIAL_STEP ${1:Name}:",
			"\t$0",
			"END_STEP;",
			""
		],
		"description": "Define Initial STEP"
    },
	"TRANSITION …": {
		"prefix": "transaction",
		"body": [
			"TRANSITION ${1:Name} FROM ${2:Step1} TO ${3:Step2}",
			"\t$0",
			"END_TRANSITION;",
			""
		],
		"description": "Define ACTION"
    },
	"FOR …": {
		"prefix": "for ",
		"body": [
			"FOR ${1:Var} TO ${2:Num} DO",
			"\t$0",
			"END_FOR;",
			""
